{
  "presets": ["flow"],
  "plugins": ["transform-object-rest-spread", "transform-runtime"],
  "env": {
    "production": {
      "presets": ["env"],
      "ignore": ["**/*.spec.js"]
    },
    "lambda": {
      "presets": [["env", { "targets": { "node": "6.10.3" } }]]
    },
    "test": {
      "presets": [["env", { "targets": { "node": "current" } }]]
    }
  }
}
