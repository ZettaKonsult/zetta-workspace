{
  "name": "membrum-subscription",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZettaKonsult/zk-open-modules.git"
  },
  "main": "dist/index.js",
  "author": "Fredrik Palmquist <fiddep@telia.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index",
    "test": "jest  --env=node",
    "test:watch": "yarn test --watch",
    "test:staged": "cross-env CI=true yarn test --findRelatedTests",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "build:watch": "yarn build -w",
    "lint": "eslint src/**/*.js",
    "travis:test": "jest",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build",
    "postpublishOnly": "rm -rf dist"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "cuid": "2.0.2",
    "zk-dynamodb-wrapper": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "5.1.3",
    "eslint": "^4.12.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "flow-bin": "0.64.0",
    "husky": "^0.14.3",
    "jest": "22.1.4",
    "jest-cli": "22.1.4",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2"
  },
  "lint-staged": {
    "*.{js}": ["eslint --fix", "prettier --write", "yarn test:staged"],
    "*.json": ["prettier --write", "git add"],
    "yarn.lock": ["git rm --cached"]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
