{
  "name": "subscription",
  "version": "0.1.0",
  "main": "dist/index.js",
  "author": "Zimon Kuhs <zmk.zk.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node src/index",
    "test": "jest src --color",
    "test:watch": "yarn run test --watch",
    "build": "babel src -d dist",
    "lint": "eslint src/**/*.js",
    "travis:test": "jest src",
    "prepublish": "yarn run build",
    "postpublish": "rm -rf /dist",
    "precommit": "lint-staged && yarn run test"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.12.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.3.1",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "jest": "22.0.3",
    "jest-cli": "22.0.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.8.2"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --trailing-comma es5 --single-quote --no-semi --write",
      "yarn run lint",
      "git add"
    ],
    "*.json": ["prettier --write", "git add"],
    "yarn.lock": ["git rm --cached"]
  },
  "jest": {
    "modulePathIgnorePatterns": ["old/*"],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.180.0",
    "babel-runtime": "^6.26.0",
    "date-primitive-utils": "^1.1.1",
    "zk-dynamodb-wrapper": "^1.0.5"
  }
}
