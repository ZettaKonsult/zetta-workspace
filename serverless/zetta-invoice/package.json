{
  "name": "zetta-invoice",
  "version": "1.0.0",
  "description": "Module managing card payments and invoices.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZettaKonsult/zetta-workspace.git"
  },
  "main": "dist/index.js",
  "author": "Fredrik Palmquist <fiddep@telia.com>",
  "contributors": [
    {
      "name": "Zimon Kuhs",
      "email": "zmk.zk.dev@gmail.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "cross-env BABEL_ENV=lambda babel src -d dist",
    "build:watch": "yarn build -w",
    "deploy:debug": "sls deploy -v",
    "db:remove": "sls dynamodb remove",
    "deploy:prod": "sls deploy --stage prod",
    "prestart": "yarn sls dynamodb install",
    "start": "yarn sls offline start",
    "dev:test": "yarn test",
    "test": "jest src --env=Node",
    "test:mail": "jest mail --env=Node",
    "test:staged": "cross-env CI=true yarn test --findRelatedTests",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.192.0",
    "babel-runtime": "^6.26.0",
    "cuid": "^2.1.0",
    "date-primitive-utils": "^1.1.1",
    "mustache": "^2.3.0",
    "nodemailer": "^4.4.2",
    "puppeteer": "^1.1.0",
    "query-string": "^5.1.0",
    "source-map-support": "^0.4.18"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "jest": "^22.2.2",
    "serverless": "^1.26.0",
    "serverless-dynamodb-local": "^0.2.28",
    "serverless-offline": "3.17.0",
    "serverless-offline-scheduler": "^0.3.3",
    "serverless-webpack": "^4.2.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
